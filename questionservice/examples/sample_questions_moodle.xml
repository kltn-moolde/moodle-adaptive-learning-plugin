  <!--Câu 3-->
  <question type="multichoice">
    <name>
      <text>Câu hỏi 3: List trong Python</text>
    </name>
    <questiontext format="html">
      <text><![CDATA[<p>Cách nào để tạo một list trong Python?</p>]]></text>
    </questiontext>
    <shuffleanswers>0</shuffleanswers>
    <single>true</single>
    <answernumbering>abc</answernumbering>
    <answer fraction="100">
      <text>my_list = []</text>
      <feedback>
        <text>Đúng! Đây là cách tạo list trống trong Python.</text>
      </feedback>
    </answer>
    <answer fraction="0">
      <text>my_list = ()</text>
      <feedback>
        <text>Sai. Đây là cách tạo tuple, không phải list.</text>
      </feedback>
    </answer>
    <answer fraction="0">
      <text>my_list = {}</text>
      <feedback>
        <text>Sai. Đây là cách tạo dictionary, không phải list.</text>
      </feedback>
    </answer>
    <answer fraction="0">
      <text>my_list = new List()</text>
      <feedback>
        <text>Sai. Python không dùng từ khóa 'new'.</text>
      </feedback>
    </answer>
