  <!--Câu 5-->
  <question type="multichoice">
    <name>
      <text>Câu hỏi 5: Dictionary</text>
    </name>
    <questiontext format="html">
      <text><![CDATA[<p>Cách nào để truy cập giá trị của key 'name' trong dictionary <code>person = {'name': 'John', 'age': 30}</code>?</p>]]></text>
    </questiontext>
    <shuffleanswers>0</shuffleanswers>
    <single>true</single>
    <answernumbering>abc</answernumbering>
    <answer fraction="100">
      <text>person['name']</text>
      <feedback>
        <text>Đúng! Đây là cách truy cập giá trị theo key trong dictionary.</text>
      </feedback>
    </answer>
    <answer fraction="0">
      <text>person.name</text>
      <feedback>
        <text>Sai. Cú pháp này dùng cho object attributes, không phải dictionary.</text>
      </feedback>
    </answer>
    <answer fraction="0">
      <text>person(name)</text>
      <feedback>
        <text>Sai. Đây không phải cú pháp hợp lệ trong Python.</text>
      </feedback>
    </answer>
    <answer fraction="0">
      <text>person-&gt;name</text>
      <feedback>
        <text>Sai. Đây là cú pháp của C/C++, không phải Python.</text>
      </feedback>
    </answer>
