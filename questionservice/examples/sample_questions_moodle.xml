  <!--Câu 2-->
  <question type="multichoice">
    <name>
      <text>Câu hỏi 2: Biến trong Python</text>
    </name>
    <questiontext format="html">
      <text><![CDATA[<p>Cách nào sau đây là đúng để khai báo biến trong Python?</p>]]></text>
    </questiontext>
    <shuffleanswers>0</shuffleanswers>
    <single>true</single>
    <answernumbering>abc</answernumbering>
    <answer fraction="100">
      <text>x = 10</text>
      <feedback>
        <text>Đúng rồi! Python không cần khai báo kiểu dữ liệu.</text>
      </feedback>
    </answer>
    <answer fraction="0">
      <text>int x = 10;</text>
      <feedback>
        <text>Sai. Đây là cú pháp của Java/C++, không phải Python.</text>
      </feedback>
    </answer>
    <answer fraction="0">
      <text>var x = 10;</text>
      <feedback>
        <text>Sai. Đây là cú pháp của JavaScript.</text>
      </feedback>
    </answer>
    <answer fraction="0">
      <text>dim x as integer</text>
      <feedback>
        <text>Sai. Đây là cú pháp của VB.</text>
      </feedback>
    </answer>
